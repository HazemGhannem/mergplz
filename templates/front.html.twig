type: "GET",
url: "{{ path('ajax_searchP') }}",
data: {
'q': value
},
dataType: "text",
success: function (msg) { // we need to check if the value is the same
if (value === $(that).val()) {
var result = JSON.parse(msg);
$.each(result, function (key, arr) {
$.each(arr, function (id, value) {
if (key === 'plats') {
if (id !== 'error') {
console.log(value[1]);
entitySelector.append('<li><b>' + value[1] + '</b><a href="/profile/Search/' + id + '">' + '<img src="/uploads/' + value[0] + '" style="width: 50px; height: 50px"/>' + '</a></li>');
